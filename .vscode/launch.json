  "version": "0.2.0",
  "configurations": [

    {
      "type": "pwa-node",
      "request": "launch",
      "name": "run example/ts",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/dist/cli/cli.js",
      "args": [
        "--current-dir",
        "examples/ts"
      ],
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    }
  ]
}